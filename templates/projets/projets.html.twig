{% extends 'base.html.twig' %}

{% block title %}Projets - FAO TRAVEL !{% endblock %}
{% block body %}
    <div class="row ">
        {% for projet in projets %}
        <div class="col-sm-4">
            <div class="card m-3  bg-secondary">
                <div class="card-body">
                    <p class="card-text">{{ projet.titre }}</p>
                </div>
                <img src="{{projet.image }}" style="width: 100%; height: 250px;" alt="{{ projet.titre }}">
            </div>
        </div>
        {% endfor %}
    </div>

{% endblock %}
