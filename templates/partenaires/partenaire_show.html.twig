{% extends 'base.html.twig' %}

{% block title %} {{ partenaire.entreprise }} - FAO TRAVEL !{% endblock %}
{% block body %}
    <div class="row">
        <div class="col-lg-2">
            <img class="rounded img-thumbnail" src="/{{ partenaire.logo }}" alt="Logo de {{ partenaire.logo }} ">
        </div>
        <div class="col-lg-10">
            <h3>{{ partenaire.entreprise }}</h3>
            <p>{{ partenaire.description | raw}}</p>
        </div>
    </div>
    <div class="row">
   <a href="{{ path('app_partenaires') }}" class="btn btn-success mt-3 ">Retour aux partenaires</a>

    </div>

{% endblock %}
